{
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:react/recommended"
  ],
  
    "parser": "@typescript-eslint/parser"
  ,
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "tsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}